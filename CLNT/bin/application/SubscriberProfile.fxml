            <Label layoutX="18.0" layoutY="267.0" prefHeight="17.0" prefWidth="82.0" text="Subscriber ID:" />
            <Label fx:id="IDLabel" layoutX="183.0" layoutY="263.0" prefHeight="24.0" prefWidth="250.0" />
            <TextField fx:id="PhoneText" disable="true" layoutX="181.0" layoutY="320.0" prefHeight="25.0" prefWidth="254.0" />
