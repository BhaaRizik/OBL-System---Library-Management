                <TableColumn fx:id="DateCol" prefWidth="143.0" text="Date" />
                <TableColumn fx:id="ActionCol" prefWidth="321.0" text="Action" />
              </columns>
            </TableView>
            <Label layoutX="461.0" layoutY="286.0" prefHeight="17.0" prefWidth="93.0" text="Histroy Actions:" />
            <Button fx:id="viewOrder" layoutX="499.0" layoutY="150.0" mnemonicParsing="false" onAction="#viewOrdersSubscriber" text="View Orders" />
