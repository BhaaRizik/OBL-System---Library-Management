<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SubscriberProfileController">
   <center>
      <AnchorPane prefHeight="597.0" prefWidth="935.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="Title" layoutX="12.0" layoutY="142.0" prefHeight="40.0" prefWidth="245.0" text="Personal information" textAlignment="CENTER">
