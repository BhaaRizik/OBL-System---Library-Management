            </Label>
            <Label layoutX="18.0" layoutY="227.0" prefHeight="17.0" prefWidth="102.0" text="SubScriber Name :" />
            <Label layoutX="20.0" layoutY="361.0" prefHeight="17.0" prefWidth="39.0" text="E-mail:" />
            <Label layoutX="19.0" layoutY="324.0" prefHeight="17.0" prefWidth="97.0" text="Phone Number:" />
            <Label layoutX="17.0" layoutY="294.0" prefHeight="17.0" prefWidth="109.0" text="Subscriber Number:" />
            <Button fx:id="LogoutLabel" layoutX="15.0" layoutY="62.0" mnemonicParsing="false" onAction="#LogoutMeth" prefHeight="19.0" prefWidth="60.0" text="Logout" />
